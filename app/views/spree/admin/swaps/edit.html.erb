<%= render :partial => 'spree/admin/shared/deli_sub_menu' %>

<% content_for :page_title do %>
    <%= Spree.t(:editing_swap) %>
<% end %>

<% content_for :page_actions do %>
    <li>
      <%= button_link_to Spree.t(:back_to_swaps_list), spree.admin_swaps_path, :icon => 'icon-arrow-left' %>
    </li>
<% end %>

<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @swap } %>

<%= form_for [:admin, @swap] do |f| %>
    <fieldset class="no-border-top">
      <%= render :partial => 'form', :locals => { :f => f } %>
      <%= render :partial => 'spree/admin/shared/edit_resource_links' %>
    </fieldset>
<% end %>
